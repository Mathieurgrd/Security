<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textViewPosition"
        android:layout_width="21dp"
        android:layout_height="28dp"
        android:layout_marginLeft="18dp"
        android:layout_marginTop="26dp"
        android:text="1"
        android:textColor="#000000"
        android:textSize="18dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textViewNameCompetition"
        android:layout_width="164dp"
        android:layout_height="28dp"
        android:layout_marginTop="26dp"
        android:gravity="center"
        android:textColor="#000000"
        android:textSize="18dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/textViewPosition"
        android:layout_marginLeft="17dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="26dp"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textViewChampionShipName"
        android:layout_width="29dp"
        android:layout_height="26dp"
        android:layout_marginTop="26dp"
        android:text="200"
        android:textColor="#000000"
        android:textSize="18dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/textViewNameCompetition"
        android:layout_marginLeft="26dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="65dp"
        android:layout_height="26dp"
        android:layout_marginTop="26dp"
        android:text="points"
        android:textColor="#000000"
        android:textSize="18dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/textViewChampionShipName"
        android:layout_marginLeft="0dp" /><![CDATA[


        app:layout_constraintLeft_toRightOf="@+id/textViewPlayerRank"
        android:layout_marginLeft="18dp" />

]]>
</android.support.constraint.ConstraintLayout>